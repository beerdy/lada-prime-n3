<p id="notice"><%= notice %></p>

<h1>Properties</h1>

<table>
  <thead>
    <tr>
      <th>Secure</th>
      <th>Interier</th>
      <th>Comfort</th>
      <th>Extarier</th>
      <th>Link</th>
      <th>Url</th>
      <th>Sort</th>
      <th>Show</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @properties.each do |property| %>
      <tr>
        <td><%= property.secure %></td>
        <td><%= property.interier %></td>
        <td><%= property.comfort %></td>
        <td><%= property.extarier %></td>
        <td><%= property.link %></td>
        <td><%= property.url %></td>
        <td><%= property.sort %></td>
        <td><%= property.show %></td>
        <td><%= link_to 'Show', property %></td>
        <td><%= link_to 'Edit', edit_property_path(property) %></td>
        <td><%= link_to 'Destroy', property, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Property', new_property_path %>
